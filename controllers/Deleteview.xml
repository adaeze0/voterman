    <field name="SpeditionID" type="Int32" allowNulls="false" label="Spedition ID">
      <items style="Lookup" dataController="Editions" newDataView="createForm1" dataValueField="EditionID" dataTextField="EditionID" copy="Editions1EditionName=EditionName" />
    </field>
    <field name="TpvoteID" type="Int32" allowNulls="false" label="Tpvote ID" />
    <field name="TpuserID" type="Int32" allowNulls="false" label="Tpuser ID">
      <items style="Lookup" dataController="Users" newDataView="createForm1" dataValueField="UserID" dataTextField="UserID" copy="Users2UserName=UserName" />
    </field>
    <field name="TpprojectID" type="Int32" allowNulls="false" label="Tpproject ID">
      <items style="Lookup" dataController="Projects" newDataView="createForm1" dataValueField="ProjectID" dataTextField="ProjectID" copy="Projects2ProjectName=ProjectName" />
    </field>
    <field name="SpvoteDate" type="DateTime" dataFormatString="g" label="Spvote Date" />
    <field name="TpvoteDate" type="DateTime" dataFormatString="g" label="Tpvote Date" />
    <field name="TpeditionID" type="Int32" allowNulls="false" label="Tpedition ID">
      <items style="Lookup" dataController="Editions" newDataView="createForm1" dataValueField="EditionID" dataTextField="EditionID" copy="Editions2EditionName=EditionName" />
    </field>
  </fields>

    <field name="SecondPlaceVotes" type="String" readOnly="true" label="Projects 2 Project Name" length="50" />
    <field name="ThirdPlaceVotes" type="String" readOnly="true" label="Projects 1 Project Name" length="50" />
    <field name="Editions1EditionName" type="String" readOnly="true" label="Editions 1 Edition Name" length="50" />
    <field name="Editions2EditionName" type="String" readOnly="true" label="Editions 2 Edition Name" length="50" />
    <field name="FpvoteID" type="Int32" allowNulls="false" label="Fpvote ID" showInSummary="true" />
    <field name="FirstPlaceVote" type="String" readOnly="true" label="Projects Project Name" length="50" />
    <field name="FpuserID" type="Int32" allowNulls="false" label="Fpuser ID" showInSummary="true">
      <items style="Lookup" dataController="Users" newDataView="createForm1" dataValueField="UserID" dataTextField="UserID" copy="UsersUserName=UserName" />
    </field>
    <field name="Fpdate" type="DateTime" dataFormatString="g" label="Fpdate" showInSummary="true" />
    <field name="SpuserID" type="Int32" allowNulls="false" label="Spuser ID" showInSummary="true">
      <items style="Lookup" dataController="Users" newDataView="createForm1" dataValueField="UserID" dataTextField="UserID" copy="Users1UserName=UserName" />
    </field>
    <field name="SpvoteID" type="Int32" allowNulls="false" label="Spvote ID" showInSummary="true" />
    <field name="FpprojectID" type="Int32" allowNulls="false" label="Fpproject ID">

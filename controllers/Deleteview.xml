<?xml version="1.0" encoding="utf-8"?>
<dataController name="Deleteview" conflictDetection="overwriteChanges" label="Deleteview" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Users"."UserName" "UsersUserName"
	,"Users1"."UserName" "Users1UserName"
	,"Users2"."UserName" "Users2UserName"
	,"Editions"."EditionName" "EditionsEditionName"
	,"Projects"."ProjectName" "FirstPlaceVote"
	,"Projects2"."ProjectName" "SecondPlaceVotes"
	,"Projects1"."ProjectName" "ThirdPlaceVotes"
	,"Editions1"."EditionName" "Editions1EditionName"
	,"Editions2"."EditionName" "Editions2EditionName"
	,"deleteview"."FPVoteID" "FpvoteID"
	,"deleteview"."FPUserID" "FpuserID"
	,"deleteview"."FPDate" "Fpdate"
	,"deleteview"."SPUserID" "SpuserID"
	,"deleteview"."SPVoteID" "SpvoteID"
	,"deleteview"."FPProjectID" "FpprojectID"
	,"deleteview"."FPEditionID" "FpeditionID"
	,"deleteview"."SPProjectID" "SpprojectID"
	,"deleteview"."SPEditionID" "SpeditionID"
	,"deleteview"."TPVoteID" "TpvoteID"
	,"deleteview"."TPUserID" "TpuserID"
	,"deleteview"."TPProjectID" "TpprojectID"
	,"deleteview"."SPVoteDate" "SpvoteDate"
	,"deleteview"."TPVoteDate" "TpvoteDate"
	,"deleteview"."TPEditionID" "TpeditionID"
from "dbo"."deleteview" "deleteview"
	left join "dbo"."Users" "Users" on "deleteview"."FPUserID" = "Users"."UserID"
	left join "dbo"."Users" "Users1" on "deleteview"."SPUserID" = "Users1"."UserID"
	left join "dbo"."Users" "Users2" on "deleteview"."TPUserID" = "Users2"."UserID"
	left join "dbo"."Projects" "Projects" on "deleteview"."SPProjectID" = "Projects"."ProjectID"
	left join "dbo"."Editions" "Editions" on "deleteview"."FPEditionID" = "Editions"."EditionID"
	left join "dbo"."Projects" "Projects1" on "deleteview"."FPProjectID" = "Projects1"."ProjectID"
	left join "dbo"."Projects" "Projects2" on "deleteview"."TPProjectID" = "Projects2"."ProjectID"
	left join "dbo"."Editions" "Editions1" on "deleteview"."SPEditionID" = "Editions1"."EditionID"
	left join "dbo"."Editions" "Editions2" on "deleteview"."TPEditionID" = "Editions2"."EditionID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="UsersUserName" type="String" readOnly="true" label="Users User Name" length="128" />
    <field name="Users1UserName" type="String" readOnly="true" label="Users 1 User Name" length="128" />
    <field name="Users2UserName" type="String" readOnly="true" label="Users 2 User Name" length="128" />

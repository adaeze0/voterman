    </field>
    <field name="FpeditionID" type="Int32" allowNulls="false" label="Fpedition ID">
      <items style="Lookup" dataController="Editions" newDataView="createForm1" dataValueField="EditionID" dataTextField="EditionID" copy="EditionsEditionName=EditionName" />
